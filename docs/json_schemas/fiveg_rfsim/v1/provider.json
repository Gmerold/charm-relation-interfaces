          "examples": [
            "192.168.70.130"
          ],
          "title": "Rfsim Address",
          "type": "string"
        },
        "sst": {
          "description": "Slice/Service Type",
          "examples": [
            1,
            2,
            3,
            4
          ],
          "maximum": 255,
          "minimum": 0,
          "title": "Sst",
          "type": "integer"
        },
        "sd": {
          "anyOf": [
            {
              "maximum": 16777215,
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "null"
            }
          ],
          "default": null,
          "description": "Slice Differentiator",
          "examples": [
            1
          ],
          "title": "Sd"
        },
        "band": {
          "default": null,
          "description": "Frequency band",
          "examples": [
            34,
            77,
            102
          ],
          "title": "Band",
          "type": "integer"
        },
        "dl_freq": {
          "default": null,
          "description": "Downlink frequency in Hz",
          "examples": [
            4059090000
          ],
          "title": "Dl Freq",
          "type": "integer"
        },
        "carrier_bandwidth": {
